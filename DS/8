#include <stdio.h>

void insert(int arr[], int *n, int x) {
    arr[*n] = x;
    (*n)++;
}

int extractMax(int arr[], int *n) {
    int maxIndex = 0;
    for(int i=1; i<*n; i++) {
        if(arr[i] > arr[maxIndex]) maxIndex = i;
    }
    int maxVal = arr[maxIndex];
    for(int i=maxIndex; i<*n-1; i++) arr[i] = arr[i+1];
    (*n)--;
    return maxVal;
}

int main() {
    int pq[10], size=0;
    insert(pq, &size, 10);
    insert(pq, &size, 30);
    insert(pq, &size, 20);
    printf("Max element: %d\n", extractMax(pq, &size));
    printf("Max element: %d\n", extractMax(pq, &size));
    return 0;
}
