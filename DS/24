#include <stdio.h>
#define SIZE 10
int hashTable[SIZE];

int hash(int key) { return key % SIZE; }

void insert(int key) {
    int idx = hash(key);
    while(hashTable[idx] != 0) idx = (idx+1)%SIZE;
    hashTable[idx] = key;
}

int search(int key) {
    int idx = hash(key), start=idx;
    while(hashTable[idx] != 0) {
        if(hashTable[idx] == key) return idx;
        idx = (idx+1)%SIZE;
        if(idx == start) break;
    }
    return -1;
}

int main() {
    for(int i=0;i<SIZE;i++) hashTable[i]=0;
    insert(10); insert(20); insert(30);
    int key=20;
    int res=search(key);
    if(res!=-1) printf("Found %d at index %d\n", key,res);
    else printf("%d not found\n", key);
    return 0;
}
