#include <stdio.h>
#define SIZE 5

int queue[SIZE];
int front = -1, rear = -1;

void enqueue(int x) {
    if((front == 0 && rear == SIZE-1) || (rear == (front-1)%(SIZE-1)))
        printf("Queue Full\n");
    else {
        if(front == -1) front = rear = 0;
        else if(rear == SIZE-1 && front != 0) rear = 0;
        else rear++;
        queue[rear] = x;
    }
}

void dequeue() {
    if(front == -1) printf("Queue Empty\n");
    else {
        printf("Dequeued: %d\n", queue[front]);
        if(front == rear) front = rear = -1;
        else if(front == SIZE-1) front = 0;
        else front++;
    }
}

void display() {
    if(front == -1) { printf("Queue Empty\n"); return; }
    printf("Queue: ");
    int i = front;
    while(1) {
        printf("%d ", queue[i]);
        if(i == rear) break;
        i = (i + 1) % SIZE;
    }
    printf("\n");
}

int main() {
    enqueue(10);
    enqueue(20);
    enqueue(30);
    display();
    dequeue();
    display();
    return 0;
}
