def fahrenheit_to_celsius(f_temp):
    """Converts temperature from Fahrenheit to Celsius."""
    # Use 5.0/9.0 or (5/9) to ensure floating-point division
    c_temp = (f_temp - 32) * (5.0 / 9.0)
    return round(c_temp, 2) # Round to 2 decimal places for clean output

# Example usage
f_reading = 68  # 68 degrees Fahrenheit is 20 degrees Celsius
c_reading = fahrenheit_to_celsius(f_reading)

print(f"{f_reading}°F is equal to {c_reading}°C")
