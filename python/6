import math

def is_prime(n):
    """Checks if a number n is prime."""
    if n <= 1:
        return False
    # Only need to check divisors up to the square root of n
    for i in range(2, int(math.sqrt(n)) + 1):
        if n % i == 0:
            return False
    return True

# Example usage
number1 = 13
number2 = 15
print(f"{number1} is prime: {is_prime(number1)}")
print(f"{number2} is prime: {is_prime(number2)}")
